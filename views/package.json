    "react sample",
    "react starter",
    "react boilerplate"
  ],
  "author": "Temi Lajumoke <temi@temilajumoke.com> (http://temilajumoke.com)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "qs": "6.5.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-flexview": "^4.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-tabs": "^2.2.2",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
