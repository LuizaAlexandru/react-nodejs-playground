{
  "name": "react-webpack-kit",
  "version": "1.0.0",
  "description": "a boiler plate for creating react applications bundled by webpack",
  "main": "src/js/app.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
